{
  "name": "blueprint",
  "version": "1.0.0",
  "description": "Visual scripting IDE",
  "main": "main.js",
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --progress --profile",
    "build": "npm run build:dev",
    "build:dev": "webpack --progress --profile",
    "package": "node package.js",
    "package-all": "npm run package -- --all",
    "start": "electron src/app",
    "start:dev": "set env=dev&&npm start",
    "server": "webpack-dev-server --inline --progress --port 8086",
    "webpack-test": "webpack --config webpack.test.js --progress --profile --content-base ./src"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "blueprint",
    "visual",
    "ide"
  ],
  "author": "odin3",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.0.3",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.3",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.4",
    "@types/electron": "^1.4.37",
    "angular-draggable": "^1.0.1",
    "angular-resizable-element": "^1.1.0",
    "angular2-draggable": "^1.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "electron": "~1.6.2",
    "electron-devtools-installer": "^2.2.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "mdi": "^1.9.33",
    "rxjs": "^5.2.0",
    "webpack-target-electron-renderer": "^0.4.0",
    "zone.js": "^0.8.5"
  },
