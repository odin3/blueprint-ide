    "imports-loader": "^0.7.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.0.0",
    "phantomjs-polyfill": "0.0.2",
